{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "minItems": 1,
    "items": {
        "type": "object",
        "required": [
            "name",
            "description",
            "resourceId"
        ],
        "additionalProperties": false,
        "properties": {
            "name": {
                "type": "string",
                "minLength": 1
            },
            "description": {
                "type": "string",
                "minLength": 1
            },
            "resourceId": {
                "type": "string",
                "pattern": "^[a-zA-Z0-9_]+$",
                "minLength": 1
            }
        }
    }
}